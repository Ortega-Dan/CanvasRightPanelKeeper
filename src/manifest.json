{
  "manifest_version": 3,
  "name": "Canvas Right Panel Keeper",
  "description": "Canvas Right Panel Keeper",
  "version": "1.0.5",
  "icons": {
    "16": "canvas16.png",
    "48": "canvas48.png",
    "128": "canvas128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://byui.instructure.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_icon": "canvas16.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": {}
}