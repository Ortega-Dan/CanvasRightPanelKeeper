{
  "manifest_version": 2,
  "name": "Canvas Right Panel Keeper",
  "description": "Canvas Right Panel Keeper",
  "version": "1.0.2",
	"browser_action": {
    	"default_icon": "canvas16.png",
    	"default_popup": "popup.html"
    },
    "icons": { "16": "canvas16.png",
              "48": "canvas48.png",
            "128": "canvas128.png" },
  "content_scripts": [
    {
      "matches": [
        "*://byui.instructure.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}